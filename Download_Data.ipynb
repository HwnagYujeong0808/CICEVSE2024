    "        print(f\"Downloading {href}...\")\n",
    "        file_response = requests.get(file_url)\n",
    "        with open(os.path.join('Dataset', href), 'wb') as f:\n",
    "            f.write(file_response.content)\n",
    "        print(f\"Downloaded {href}\")\n",
    "\n",
    "print(\"All files downloaded.\")"
   ]
